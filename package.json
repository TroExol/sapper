{
  "name": "sapper",
  "version": "0.1.0",
  "private": true,
  "author": "Иван Клочковский",
  "main": "./src/index.js",
  "dependencies": {
    "jss": "^10.9.2",
    "jss-plugin-default-unit": "^10.9.2",
    "jss-plugin-vendor-prefixer": "^10.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.9.2"
  },
  "scripts": {
    "postinstall": "git config core.hooksPath ./git-hooks",
    "start": "webpack-dev-server --watch --progress --mode=development --inline --hot --config webpack/local.js",
    "build": "webpack --mode=production --config webpack/production.js",
    "linter": "eslint src/ --ext .js --ext .jsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "2.0.2",
    "css-loader": "2.1.1",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "favicons": "^7.0.1",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.5.2",
    "minimist": "^1.2.8",
    "prop-types": "^15.8.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.7",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "^Util(.*)": "./src/utils\\1",
            "^Component(.*)": "./src/components\\1",
            "^Hook": "./src/hooks\\1",
            "^Settings": "./src/settings.js"
          }
        }
      ]
    ]
  }
}
